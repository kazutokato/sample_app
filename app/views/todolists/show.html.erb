<h2>Title</h2>
<p><%= @list.title %></p>
<h2>Body</h2>
<p><%= @list.body %></p>
<h2>Image</h2>
<%= attachment_image_tag @list, :image, :fill, 300, 300, format: 'jpeg' %>
<%= link_to "Edit", edit_tudolist_path(@list.id) %>
<%= link_to "Destroy", destroy_todolist_path(@list.id), method: :delete, "data-confirm" => "本当に削除しますか？" %>